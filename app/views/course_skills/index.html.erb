<p id="notice"><%= notice %></p>

<h1>Course Skills</h1>

<%= link_to 'New Course Skill', new_course_skill_path %>

<table>
  <thead>
    <tr>
      <th>Course</th>
      <th>Skill</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_skills.each do |course_skill| %>
      <tr>
        <td><%= course_skill.course_title %></td>
        <td><%= course_skill.skill_name %></td>
        <td><%= link_to 'Show', course_skill %></td>
        <td><%= link_to 'Edit', edit_course_skill_path(course_skill) %></td>
        <td><%= link_to 'Destroy', course_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Course Skill', new_course_skill_path %>
